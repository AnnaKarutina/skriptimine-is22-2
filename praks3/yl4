#echo "$reisijad"
#echo "$bussis_kohti"

echo -n "Sisesta reisijate arv: "
read reisijad

echo -n "Sisesta bussis kohtade arv: "
read bussis_kohti


bussid=$(( $reisijad / $bussis_kohti ))
echo "T채ielikult t채idetud bussid - $bussid"

maha_jaanud=$(( $reisijad % $bussis_kohti ))
echo "Maha j채채nud on $maha_jaanud inimest"
